{
  "name": "make-money",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tb": "sudo yarn tauri build",
    "tauri-build-mac-intel": "sudo yarn tauri build --target x86_64-apple-darwin",
    "tauri-build-mac-m": "sudo yarn tauri build --target aarch64-apple-darwin",
    "tauri-build-windows": "sudo yarn tauri build --runner cargo-xwin --target x86_64-pc-windows-msvc",
    "tarui-build-all": "yarn tauri-build-mac-intel && yarn tauri-build-mac-m && yarn tauri-build-windows",
    "rm-target": "sudo rm -rf src-tauri/target"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.2.0",
    "antd": "^5.2.2",
    "boring-avatars": "^1.7.0",
    "i18next": "^23.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.1.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.2.2",
    "@types/node": "^18.7.10",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^3.0.0",
    "typescript": "^4.6.4",
    "vite": "^4.0.0"
  }
}